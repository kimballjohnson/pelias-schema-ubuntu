{
  "esclient": {
    "hosts": [
      {
        "env": "production",
        "protocol": "http",
        "host": "localhost",
        "port": 9200
      },
      {
        "env": "production",
        "protocol": "http",
        "host": "localhost",
        "port": 9300
      }
    ]
  },
  "imports": {
    "geonames": {
      "datapath": "~/geonames",
      "import": [
        { "type": "geoname", "filename": "GB.zip" }
      ]
    },
    "quattroshapes": {
      "datapath": "~/quattroshapes"
    },
    "openstreetmap": {
      "datapath": "~/openstreetmap",
      "leveldbpath": "/tmp/leveldb",
      "import": [{
        "type": { "node": "osmnode", "way": "osmway" },
        "filename": "london.osm.pbf"
      }]
    }
  }
}