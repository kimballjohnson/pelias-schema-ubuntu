{
  "esclient": {
    "hosts": [
      {
        "env": "production",
        "protocol": "http",
        "host": "localhost",
        "port": 9200
      },
      {
        "env": "production",
        "protocol": "http",
        "host": "localhost",
        "port": 9300
      }
    ]
  },
  "imports": {
    "geonames": {
      "datapath": "~/geonames"
    },
    "quattroshapes": {
      "datapath": "~/quattroshapes"
    },
    "openstreetmap": {
      "datapath": "~/openstreetmap",
      "leveldbpath": "/tmp/leveldb",
      "import": [{
        "type": { "node": "osmnode", "way": "osmway" },
        "filename": "london.osm.pbf"
      }]
    },
    "openaddresses": {
      "datapath": "~/openaddresses",
      "files": [
        "us-ny-nyc.csv"
      ]
    }
  }
}